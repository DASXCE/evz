<html xmlns="http://www.w3.org/1999/xhtml" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter" xmlns:j="tapestry-library:jquery">
	<head>
		<meta http-equiv="content-type" content="text/html; charset=utf-8" />
		<title>${title}</title>
		<script type="text/javascript">
		$(document).ready(function() {
	$(".bb").fadein();
	});
	</script>
	</head>
	<body class="bb">
		<!-- start header -->
		<div class="header">
			<div class="logo">
				<h1>
					Korisnik: Test
				</h1>
			</div>
			<div class="menu">
			<a>
			Logout
			</a>
			</div>
		</div>
		<!-- end header -->
		<!-- start page -->
		<div class="page">
			<!-- start sidebar -->
			<div class="sidebar">
				<ul>
					<li class="search" style="background: none;">
					</li>
					<li>
						<t:alerts />
					</li>
					<li t:type="if" test="sidebar">
					<!-- 	<j:accordion t:panels="Vlasnik,Gazdinstvo,
		Veterinar"
							t:activeElementId="activeElement">

							<p:Vlasnik>
							</p:Vlasnik>

							<p:Gazdinstvo>
								<h3>Element 2</h3>
								<hr />
								content from block 2
							</p:Gazdinstvo>

							<p:Veterinar>
								<h3>Element 3</h3>
								<hr />
								content from block 3
							</p:Veterinar>
						</j:accordion> -->

						<div class="sidebar-content">
							<t:delegate to="sidebar" />
						</div>
					</li> 
				</ul>
			</div>
			<!-- end sidebar -->
			<!-- start content -->
			<div class="content">
				<div class="post">
					<div class="entry">
						<t:body />
					</div>
				</div>
			</div>
			<!-- end content -->
			<br style="clear: both;" />
		</div>
		<!-- end page -->
		<!-- start footer -->
		<div class="footer">
			<p class="legal">
                &copy;2012
				fit.piris. All Rights Reserved.
                &nbsp;&nbsp;&bull;&nbsp;&nbsp;
				Design by
				<a href="http://www.freecsstemplates.org/">Free CSS Templates</a>
                &nbsp;&nbsp;&bull;&nbsp;&nbsp;
				Icons by
				<a href="http://famfamfam.com/">FAMFAMFAM</a>
				.
			</p>
		</div>
		<!-- end footer -->
	</body>
</html>
